{
  "expo": {
    "name": "MindFuel",
    "slug": "MindFuel",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo_splash_icon.webp",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo_splash_icon.webp",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/logo_splash_icon.webp"
    },
    "plugins": [
      [
        "expo-font"
      ],
      "react-native-video"
    ]
  }
}
